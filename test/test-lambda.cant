(import (use "../examples/lambda-calculus/interpreter")
  run-file)

(let test-names '("trivial1.lambda"
                  "trivial2.lambda"
                  "church.lambda"
                  "factorial.lambda"
                  "reverse.lambda"
                  "elementary-ca.lambda"
                  "turing-scott.lambda"
                  ))

(for each! ((test test-names))
  (out .say "\nTesting ~d\n" test)
  (out .print (run-file (chain "examples/lambda-calculus/examples/" test))))
